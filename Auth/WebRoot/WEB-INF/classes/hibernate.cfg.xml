<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
          "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>

<session-factory>
	<property name="dialect">
		org.hibernate.dialect.Oracle9Dialect
	</property>
	<property name="connection.url">
		jdbc:oracle:thin:@localhost:1521:orcl
	</property>
	<property name="connection.username">scott</property>
	<property name="connection.password">tiger</property>
	<property name="connection.driver_class">
		oracle.jdbc.driver.OracleDriver
	</property>
	<property name="myeclipse.connection.profile">orcl</property>
	<property name="show_sql">true</property>
	<property name="format_sql">true</property>
	<mapping resource="com/cissst/auth/vo/TbActions.hbm.xml" />
	<mapping resource="com/cissst/auth/vo/TbRole.hbm.xml" />
	<mapping resource="com/cissst/auth/vo/TbRoleAction.hbm.xml" />
	<mapping resource="com/cissst/auth/vo/TbUser.hbm.xml" />

</session-factory>

</hibernate-configuration>