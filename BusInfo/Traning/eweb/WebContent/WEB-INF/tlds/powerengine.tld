	<tag>
		<name>CSIIDigitalSignature</name>
		<tag-class>com.csii.pe.channel.http.tag.CSIIDigitalSignatureTag</tag-class>
		<body-content>JSP</body-content>

	</tag>	
	
	<!-- 
	Fields that will be sent to Web App:
	  currentIndex        begin position
	  recordNumber        total record number(to keep total record number for database query)
    example:
	  &lt;pe:combinedPage total="${recordNumber}"  &gt;
	  
	-->
	<tag>
		<name>combinedPage</name>
		<tag-class>com.csii.pe.channel.http.tag.page.CombinedPageTag</tag-class>
		<body-content>empty</body-content>
		<!-- create javascript or not -->
		<attribute>
			<name>script</name>
		</attribute>

		<!-- if pageNo is not valid, alert alertMessage -->
		<attribute>
			<name>alertMessage</name>
		</attribute>

        <!-- using assigned action to generate the action of the form -->
		<attribute>
			<name>action</name>
		</attribute>
		
		<!-- set EL expression for total record number  for example: ${recordNumber} -->
		<attribute>
			<name>total</name>
			<rtexprvalue>true</rtexprvalue>
			<required>true</required>
		</attribute>
		<!-- set EL expression for page size, for examplet: ${pageSize} , if can't get pageSize then use defaultPageSize -->		
		<attribute>
			<name>pageSize</name>
			<rtexprvalue>true</rtexprvalue>
			<required>true</required>

		</attribute>

		<!-- set name for page bean, default: _PageBean 
		
		page bean properties:
		{
		 int recordNumber;
		 int pageSize;
		 int pageNumber;
		 int pageNo;	
		 boolean prevPageEnable;
		 int prevPageIndex;
		 boolean nextPageEnable;
		 int nextPageIndex;
		 }
		
		-->				
		<attribute>
			<name>var</name>
		</attribute>
        <!-- set default page size, default: 10 -->					
		<attribute>
			<name>defaultPageSize</name>
		</attribute>
        <!-- set hidden field name for current index, default: "currentIndex" -->							
		<attribute>
			<name>hiddenFieldName</name>
		</attribute>
        <!-- set hidden field name for total record number, default: "recordNumber" -->									
		<attribute>
			<name>hiddenTotalFieldName</name>
		</attribute>
		
		
	</tag>

	<tag>
		<name>TopPage</name>
		<tag-class>com.csii.pe.channel.http.tag.page.TopPageTag</tag-class>
		<body-content>empty</body-content>
		<!-- set name for page bean, default: _PageBean -->				
		<attribute>
			<name>var</name>
		</attribute>

		<!-- type
		 0: only create onclick (default)
		 1: create button and onclick
		 2: create link and onclick
		 -->				
		<attribute>
			<name>type</name>
		</attribute>

		<!-- insertContent default: ''
		 insert css to button 
		 for example: class='button'
		 -->				
		<attribute>
			<name>insertContent</name>
		</attribute>
		<!-- value  default: 首 页-->				
