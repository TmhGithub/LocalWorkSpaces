	</tag>

	<!-- 
	 render body if the assigned role or any its members is implied by the role of current User.

    example:

	  &lt;pe:menufilter role="Query" &gt;
	   Query-> 
	  &lt;pe:menufilter role="QueryBalance" &gt;
	    QueryBlance
  	  &lt;/pe:menufilter &gt;
	  &lt;pe:menufilter role="QueryDetail" &gt;
	    QueryDetail
  	  &lt;/pe:menufilter &gt;
	   End of Query
	  &lt;/pe:menufilter &gt;
	  
	  &lt;pe:menufilter role="Transfer" &gt;
	    Transfer->
	  &lt;pe:menufilter role="InnerTransfer" &gt;
	    InnerTransfer
  	  &lt;/pe:menufilter &gt;
	  &lt;pe:menufilter role="InnerBankTransfer" &gt;
	    InnerBankTransfer
  	  &lt;/pe:menufilter &gt;
	   End of Transfer	  	  
	  &lt;/pe:menufilter &gt;

	-->

	<tag>
		<name>menufilter</name>
		<tag-class>com.csii.pe.channel.http.tag.MenuFilterTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>role</name>
		</attribute>
		<attribute>
			<name>rolePool</name>
		</attribute>
		<attribute>
			<name>var</name>
		</attribute>

	</tag>
	

	<tag>
		<name>push</name>
		<tag-class>com.csii.pe.channel.http.tag.PushTag</tag-class>
		<body-content>empty</body-content>
