		<attribute>
			<name>queueName</name>
		</attribute>
		<!-- set curent page's level -->										
		<attribute>
			<name>level</name>
		</attribute>
		
		<!-- if true, reset saved data in the assigned level. default:false -->										
		<attribute>
			<name>reset</name>
		</attribute>
		
		<!-- set fields' name to keep -->										
		<attribute>
			<name>fieldList</name>
		</attribute>
		<!-- set trsName to keep -->										
		<attribute>
			<name>trsName</name>
		</attribute>
		<!-- constants to keep  sample: name1=value1:name2=value2: -->
		<attribute>
			<name>constants</name>
		</attribute>
		
        <!-- default: false. if true, the kept data will be restored to Context when form is submitted -->		
		<attribute>
			<name>generateReqData</name>
		</attribute>
		<!-- default:currentIndex, if generateReqData is true, the fields in skipFields will not be  restored. -->
		<attribute>
			<name>skipFields</name>
		</attribute>
		
		
		
	</tag>
	
	<tag>
		<name>goBack</name>
		<tag-class>com.csii.pe.channel.http.tag.page.GoBackTag</tag-class>
		<body-content>empty</body-content>
		<!-- default: PageRoot -->												
		<attribute>
			<name>queueName</name>
		</attribute>
		<!-- set the level to go -->												
		<attribute>
			<name>level</name>
		</attribute>
		<!-- set trsName. default: null, using trsName kept in session -->												
		<attribute>
			<name>trsName</name>
		</attribute>
		<!-- use defaultScript if can't find the assigned level or queue. default: null, throw Exception -->												
		<attribute>
			<name>defaultScript</name>
		</attribute>
		
	</tag>

	<tag>
		<name>error</name>
		<tag-class>com.csii.pe.channel.http.tag.ErrorTag</tag-class>
		<body-content>empty</body-content>
		<!-- default: null -->												
		<attribute>
			<name>condition</name>
		</attribute>
		<!-- error variable. default:_exceptionMessage -->												
		<attribute>
			<name>errorName</name>
		</attribute>
	</tag>

	<tag>
		<name>wcss</name>
		<tag-class>com.csii.pe.channel.http.tag.WCSSTag</tag-class>
		<body-content>empty</body-content>
		<!-- clientInfo attribute name . dfeault: _CSIIClientInfo -->												
		<attribute>
			<name>clientInfoName</name>
		</attribute>
		<!-- cssResourcePool attribute name. default:__CSIICSSResourcePool -->												
		<attribute>
			<name>cssResourcePoolName</name>
		</attribute>
	</tag>

	
</taglib>
