	    <!-- 总记录数 -->
		<name>RecordNumber</name>
		<tag-class>com.csii.pe.channel.http.tag.page.RecordNumberTag</tag-class>
		<body-content>empty</body-content>
		<!-- set name for page bean, default: _PageBean -->				
		<attribute>
			<name>var</name>
		</attribute>		
	</tag>	
	
	<tag>

		<name>a</name>
		<tag-class>com.csii.pe.channel.http.tag.AnchorTag</tag-class>
		<body-content>JSP</body-content>
		<!-- set name for current item,, 
		     if current item is not a map, then keyName is mandatory 
		     if current item is a map and fieldList is not set, then include all pairs in map to generate the href
		      -->				
		<attribute>
			<name>value</name>
			<rtexprvalue>true</rtexprvalue>
			<required>true</required>
		</attribute>	
		<!-- set transaction id for current item, for examples: QueryCif  -->						
		<attribute>
			<name>trsName</name>
		</attribute>		
		<!-- set the list of fields that will be included in href, using commas as delimeter  -->						
		<attribute>
			<name>fieldList</name>
		</attribute>		
		<!-- set the name of key that will be showed in page, if not set and fieldlist is set, then use the first field of fieldList  -->								
		<attribute>
			<name>keyName</name>
		</attribute>					
		<!-- set content that will be appended to url  -->										
		<attribute>
			<name>append</name>
		</attribute>					
		<!--set condition to render the url -->										
		<attribute>
			<name>condition</name>
		</attribute>					
		

		<!--create onclick for ajax application: default: false -->										
		<attribute>
			<name>ajax</name>
		</attribute>	
		
		<!--ajax function: default: post2Server -->										
		<attribute>
			<name>ajaxFunction</name>
		</attribute>	
						
		<!--spanId  default: null -->										
		<attribute>
			<name>spanId</name>
		</attribute>					
		<!--form object : default: null -->										
		<attribute>
			<name>form</name>
		</attribute>					
		
		
	</tag>	

	<tag>
	  
		<name>ViewName</name>
		<tag-class>com.csii.pe.channel.http.tag.ViewNameTag</tag-class>
		<body-content>empty</body-content>
